<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <FontFamily x:Key="SegoeUI.SemiBold">/Lemon.Map.Wpf;component/Resources/Segoe UI Semibold.ttf#Segoe UI Semibold</FontFamily>
    <Style TargetType="TextBlock">
        <Setter Property="FontFamily" Value="{StaticResource SegoeUI.SemiBold}"/>
    </Style>
    <Style TargetType="TextBox">
        <Setter Property="FontFamily" Value="{StaticResource SegoeUI.SemiBold}"/>
    </Style>
</ResourceDictionary>